{% extends '::base.html.twig' %}

{% block body -%}


    {{ wo_render_breadcrumbs() }}

    {{ dump(entity) }}
    <h1>Marque</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ entity.titre }}</td>
            </tr>
            <tr>
                <th>Dateupdate</th>
                <td>{{ entity.dateUpdate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>


    {% for unTag in entity.tags %}
        {{ unTag.nom }}


    {% endfor %}

    {{ dump(entity.tags) }}


        <ul class="record_actions">
    <li>
        <a href="{{ path('marque') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('marque_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
